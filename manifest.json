{
  "manifest_version": 3,
  "name": "Search Decrapify",
  "description": "Show search results without AI crap",
  "version": "1.1",
  "homepage_url": "https://github.com/XYZ-Project",
  "author": "Protik Islam-Jakobsson",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "activeTab"
  ],
  "host_permissions": [
    "*://www.google.com/*"
  ],
  "offline_enabled": false,

  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}			
